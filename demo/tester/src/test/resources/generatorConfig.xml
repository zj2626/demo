<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<!-- 配置Run As Maven build : Goals 参数 : mybatis-generator:generate -Dmybatis.generator.overwrite=true -->
<generatorConfiguration>

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <plugin type="com.kdniao.mybatis.generator.plugin.mybatis.MysqlPaginationPlugin"></plugin>

        <!-- jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver" connectionURL="jdbc:oracle:thin:@localhost:1521:xe" userId="wechatgateway"
            password="wechatgateway"> </jdbcConnection -->

        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.1.233:3306/test"
                        userId="root" password="sql@kdn!123">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!-- generate Model -->
        <javaModelGenerator targetPackage="com.kdniao.logisticsfront.common.biz.service.impl.mybatis.dal.model" targetProject="src\test\java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!-- generate xml -->
        <sqlMapGenerator targetPackage="com.kdniao.logisticsfront.common.biz.service.impl.mybatis.dal" targetProject="src\test\resources\META-INF\mybatis">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <!-- generate Mapper -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.kdniao.logisticsfront.common.biz.service.impl.mybatis.dal.mapper"
                             targetProject="src\test\java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <table schema="" tableName="uc_area" domainObjectName="ucAreaDO">
            <property name="useActualColumnNames" value="false" />
        </table>
    </context>
</generatorConfiguration>